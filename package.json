{
  "name": "voxflow",
  "version": "1.0.0",
  "description": "VoxFlow IVR Application with drag-drop designer and voice streaming",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "cp .env.development .env && node server.js",
    "start:prod": "cp .env.production .env && NODE_ENV=production node server.js",
    "dev": "cp .env.development .env && nodemon server.js",
    "dev:prod": "cp .env.production .env && NODE_ENV=production nodemon server.js",
    "test": "jest"
  },
  "keywords": [
    "ivr",
    "voice",
    "websocket",
    "kookoo",
    "drag-drop"
  ],
  "author": "VoxFlow Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "openai": "^4.104.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
